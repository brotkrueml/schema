{
    "name": "brotkrueml/schema",
    "type": "typo3-cms-extension",
    "description": "API and view helpers for schema.org markup",
    "license": [
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "keywords": [
        "TYPO3 CMS",
        "TYPO3 extension",
        "JSON-LD",
        "Structured data",
        "SEO",
        "schema.org"
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "typo3/cms-core": "^9.5 || 10.0.*"
    },
    "require-dev": {
        "clue/graph": "^0.9",
        "dg/bypass-finals": "^1.1",
        "infection/infection": "^0.13",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.11",
        "phpunit/phpunit": "^8.1",
        "twig/twig": "^2.0",
        "typo3/testing-framework": "^4.13 || ^5.0"
    },
    "replace": {
        "typo3-ter/schema": "self.version"
    },
    "conflict": {
        "brotkrueml/sdbreadcrumb": "*"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\Schema\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\Schema\\Generator\\": "Generator/Classes/",
            "Brotkrueml\\Schema\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages" : true,
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/web"
        }
    }
}
